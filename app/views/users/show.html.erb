<div class="container">

<p id="notice"><%= notice %></p>

<p><%= image_tag @user.profile.image_url(:thumb)%></p>


<p>
  <strong>Name:</strong>
  <%= @user.profile.full_name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
